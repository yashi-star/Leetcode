class Solution {
public:
   vector<string>backtrack(string curr, int open, int close,int n , vector<string>&res){
        if(curr.length()==2*n){
         res.push_back(curr);
         return res;
        }

        if(open<n) {
            backtrack(curr+'(' , open+1, close, n,res);
        }
        if(close<open){
            backtrack(curr+')', open,close+1,n,res);
        }
        return res;
    }
    vector<string> generateParenthesis(int n) {
        vector<string> res;
        string curr="";
        return backtrack(curr,0,0,n,res);
    }
};
