class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>m;
        int n=nums.size();
        int diff=0; 

        for(int i=0;i<n;i++){
            diff=target-nums[i];
            if(m.find(diff)!=m.end()  && m[diff] !=i)
            return {i,m[diff]};

            else  m[nums[i]]=i;
        }
        return {};
    }
};
